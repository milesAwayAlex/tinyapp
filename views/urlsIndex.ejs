<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('./partials/_head'); %>
  </head>
  <body>
    <main style="margin: 1em">
      <h3>My URLs</h3>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Short URL</th>
            <th scope="col">Long URL</th>
          </tr>
        </thead>
        <tbody>
          <% Object.entries(urls).forEach(([key,value]) => { %>
          <tr>
            <td><%= key %></td>
            <td><%= value %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </main>
  </body>
</html>
